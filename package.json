    "@nestjs/swagger": "^3.1.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "config": "^3.3.1",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.22.0",
    "mongoose": "^5.9.16",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
